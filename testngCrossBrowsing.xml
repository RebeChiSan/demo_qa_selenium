<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Crossbrowsing Test Suite" parallel="false">
    <test name="Tooltips Tests - Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="Tests.Widgets.ToolTips"/>
        </classes>
    </test>
    <test name="Tooltips Tests - Firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="Tests.Widgets.ToolTips"/>
        </classes>
    </test>

    <test name="Tooltips Tests - Edge">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="Tests.Widgets.ToolTips"/>
        </classes>
    </test>
</suite>